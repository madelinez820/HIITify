{
  "name": "HIITify",
  "description": "Customize your favorite Spotify songs to match your HIIT workout with custom BPMs for workout and rest intervals and an overall workout timer.",
  "version": "0.1",
  "permissions": [
    "storage"
  ],
  "icons": {"168": "logo.png" },
  "content_scripts": [
    {
      "matches": ["https://open.spotify.com/*"],
      "js": ["content-script.js"]
    },
    {
      "matches": ["https://open.spotify.com/*"],
      "js": ["main-content-script.js"],
      "run_at": "document_end"
    }
  ],
  "manifest_version": 2
}